import "./aws_control_product.juttle" as control_product;
import "./aws_defined_metrics.juttle" as defined_metrics;

export input metric1_in: select
    -label 'CloudWatch Metric 1'
    -items defined_metrics.all_metrics[control_product.product_in]
    -default '(null)';
